@model SysPet.Models.InventariosViewModel

@{
    ViewData["Title"] = "View";
}

<div class="col-md-6 offset-3">
    <h1>Detalle de inventario</h1>

    <div>
        @Html.ActionLink("Ir a Inventarios", "Index", new {}, new { @class = "btn btn-primary" })
    </div>

    <div>
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FechaIngreso)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaIngreso)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FechaVencimiento)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaVencimiento)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Stock)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Stock)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Producto)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Producto)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Descripcion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Descripcion)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Estado)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Estado)
            </dd>
        </dl>
    </div>
    <div>
        @Html.ActionLink("Editar", "Edit", new { id=Model.Id }, new { @class = "btn btn-primary" })
        @Html.ActionLink("Eliminar", "Delete", new { id = Model.Id }, new { @class = "btn btn-danger" })
    </div>
</div>


